CREATE OR REPLACE FORCE VIEW CDM.GC_MV_0013_INDEF
(ORD, CENTRO, CONTROL, DESCR)
AS 
SELECT SUBSTR (UBI_NOMBRE, 1, 3) ORD, '<span>' || DECODE (UBI_CAH_CODIGO,  10004, 'VALME',  10192, 'TOMILLAR') || '</span>' AS CENTRO
           ,'<span alt="' || UBI_CAH_CODIGO || UBI_CODIGO || '">' || UBI_NOMBRE || '</span>' AS CONTROL, UBI_DESCRIPCION AS DESCR
       FROM GC_MV_0013
      WHERE SUBSTR (UBI_NOMBRE, 1, 1) NOT IN ('1', '2', '3', '4', '5', '6', '7', '8', '9')
   ORDER BY UBI_CAH_CODIGO, SUBSTR (UBI_NOMBRE, 1, 3) DESC
/
