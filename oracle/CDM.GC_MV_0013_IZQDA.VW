CREATE OR REPLACE FORCE VIEW CDM.GC_MV_0013_IZQDA
(ORD, CENTRO, CONTROL, DESCR)
AS 
SELECT SUBSTR (UBI_NOMBRE, 1, 1) ORD, '<span>' || DECODE (UBI_CAH_CODIGO,  10004, 'VALME',  10192, 'TOMILLAR') || '</span>' AS CENTRO
         ,'<span alt="' || UBI_CAH_CODIGO || UBI_CODIGO || '">' || UBI_NOMBRE || '</span>' AS CONTROL, UBI_DESCRIPCION AS DESCR
     FROM GC_MV_0013
    WHERE SUBSTR (UBI_NOMBRE, 1, 1) IN ('1', '2', '3', '4', '5', '6', '7', '8', '9') AND INSTR (UBI_NOMBRE, 'Izquierda') > 0
/
